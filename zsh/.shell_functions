#!/bin/bash
# This is a custom script of shell functions by Bifuz.

function z() {

  if [ $# -ne 1 ]; then

    echo "Only one argument accepted."
      echo "z <argument>"
      exit 1
    fi;

  re='^[0-9]+$'

    if ! [[ $@ =~ $re ]] ; then
      echo "Argument has to be a positive number!"
    fi;

    for i in $(seq 1 $@); do
    cd ..
  done;
}
